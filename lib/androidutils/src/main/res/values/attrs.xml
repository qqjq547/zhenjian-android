<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="WtTitleBar">
        <!--是否为浅色模式-->
        <attr name="tb_isLightMode" format="boolean" />
        <!--背景-->
        <attr name="tb_background" format="color" />
        <!--标题-->
        <attr name="tb_tvTitle_text" format="string" />
        <!--返回图片-->
        <attr name="tb_ivBack_src" format="reference" />
        <attr name="tb_ivBack_hide" format="boolean" />
        <!--右侧文字-->
        <attr name="tb_tvRight_text" format="string" />
        <attr name="tb_tvRight_textColor" format="color" />
        <attr name="tb_tvRight_background" format="reference" />
        <attr name="tb_tvRight_enable" format="boolean" />
        <!--右侧图片-->
        <attr name="tb_ivRight_src" format="reference" />
        <!--右侧spinner是否可见-->
        <attr name="tb_spinnerRight_show" format="boolean" />
    </declare-styleable>


    <declare-styleable name="TioEditText">
        <attr name="tet_clearIconEnable" format="boolean" />
        <attr name="tet_leftDrawable" format="reference" />
    </declare-styleable>


    <declare-styleable name="ListCellView">
        <!--标题-->
        <attr name="lcv_tvTitle_text" format="string" />
        <attr name="lcv_tvTitle_drawableLeft" format="reference" />
        <!--副标题-->
        <attr name="lcv_tvSubTitle_text" format="string" />
        <!--分割线-->
        <attr name="lcv_vLine_hide" format="boolean" />
        <attr name="lcv_vLine_marginLeft" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CircleProgressView">
        <attr name="cpv_progressNormalColor" format="color"/>
        <attr name="cpv_progressReachColor" format="color"/>
        <attr name="cpv_progressTextColor" format="color"/>
        <attr name="cpv_progressTextSize" format="dimension"/>
        <attr name="cpv_progressTextOffset" format="dimension"/>
        <attr name="cpv_progressNormalSize" format="dimension"/>
        <attr name="cpv_progressReachSize" format="dimension"/>
        <attr name="cpv_radius" format="dimension"/>
        <attr name="cpv_progressTextVisible" format="boolean"/>
        <attr name="cpv_progressStartArc" format="integer"/>
        <attr name="cpv_progressTextSkewX" format="dimension"/>
        <attr name="cpv_progressTextPrefix" format="string"/>
        <attr name="cpv_progressTextSuffix" format="string"/>
        <attr name="cpv_innerBackgroundColor" format="color"/>
        <attr name="cpv_progressStyle" format="enum">
            <enum name="Normal" value="0"/>
            <enum name="FillInner" value="1"/>
            <enum name="FillInnerArc" value="2"/>
        </attr>
        <attr name="cpv_innerProgressColor" format="color"/>
        <attr name="cpv_innerPadding" format="dimension"/>
        <attr name="cpv_outerColor" format="color"/>
        <attr name="cpv_outerSize" format="dimension"/>
        <attr name="cpv_reachCapRound" format="boolean"/>
    </declare-styleable>
</resources>